<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <ripple android:color="@color/originPress">
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="@color/origin"/>
                </shape>
            </item>
        </ripple>
    </item>
    <item android:drawable="@color/origin" android:state_pressed="false"/>
</selector>